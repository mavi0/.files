[init]
	defaultBranch = main
[tag]
	forceSignAnnotated = true
[credential]
	helper = osxkeychain
[commit]
	template = ~/.gitconfig.d/gitcommit
	gpgsign = true
[help]
	autoCorrect = "prompt"
[core]
	excludesfile = ~/.gitconfig.d/gitignoreglobal
	editor = "vim"
[url "https://github.com/"]
	insteadOf = gh:
[url "https://gitlab.com/"]
	insteadOf = gl:
[advice]
	detachedHead = false
[user]
	name = Eleanor Davies
	email = ellie@mavieson.co.uk
	signingkey = ssh-rsa  arisaie@zeus
[gpg]
	format = ssh
